<!DOCTYPE html>
<html>
<head>
    <title>Visitors an Admin Panel Category Bootstrap Responsive Website Template | Home :: w3layouts</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Visitors Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!-- bootstrap-css -->
    <link rel="stylesheet" href="~/Areas/Admin/Public/css/bootstrap.min.css">
    <!-- //bootstrap-css -->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css" integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" crossorigin="anonymous">
    <!-- Custom CSS -->
    <link href="~/Areas/Admin/Public/css/style.css" rel='stylesheet' type='text/css' />
    <link href="~/Areas/Admin/Public/css/style-responsive.css" rel="stylesheet" />
    <!-- font CSS -->
    <link href='//fonts.googleapis.com/css?family=Roboto:400,100,100italic,300,300italic,400italic,500,500italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>
    <!-- font-awesome icons -->
    <link rel="stylesheet" href="~/Areas/Admin/Public/css/font.css" type="text/css" />
    <link href="~/Areas/Admin/Public/css/font-awesome.css" rel="stylesheet">
    <link rel="stylesheet" href="~/Areas/Admin/Public/css/morris.css" type="text/css" />
    <!-- calendar -->
    <link rel="stylesheet" href="~/Areas/Admin/Public/css/monthly.css">
    <!-- //calendar -->
    <!-- //font-awesome icons -->
    <script src="~/Areas/Admin/Public/js/jquery2.0.3.min.js"></script>
    <script src="~/Areas/Admin/Public/js/raphael-min.js"></script>
</head>
<body>
    <section id="container">
        <!--header start-->
        <header class="header fixed-top clearfix">
            <!--logo start-->
            <div class="brand">
                <a href="/Admin/Dashboard/Index" class="logo">
                    VISITORS
                </a>
                <div class="sidebar-toggle-box">
                    <div class="fa fa-bars"></div>
                </div>
            </div>
            <!--logo end-->
            <div class="nav notify-row" id="top_menu">
                <!--  notification start -->
                <ul class="nav top-menu">
                   
                    <!-- inbox dropdown start-->
                    <li id="header_inbox_bar" class="dropdown">

                    </li>
                    <!-- inbox dropdown end -->

             
                </ul>
                <!--  notification end -->
            </div>
            <div class="top-nav clearfix">
                <!--search & user info start-->
                <ul class="nav pull-right top-menu">
                    <li>
                        <input id="usersrc" type="text" class="form-control search" placeholder=" Istifadəçi">
                    </li>
                    <!-- user login dropdown start-->
                    @{ 
                        var admin = Session["lgadmin"] as FinalProj.Models.adminSetting;
                    }
                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                            <img alt="" src="~/Areas/Admin/Public/images/3.png">
                            <span class="username">@admin.Firstname</span>
                            <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu extended logout">
                            <li><a href="/Account/Logout"><i class="fa fa-key"></i> Çıxış </a></li>
                        </ul>
                    </li>
                    <!-- user login dropdown end -->

                </ul>
                <!--search & user info end-->
            </div>
        </header>
        <!--header end-->
        <!--sidebar start-->
        <aside>
            <div id="sidebar" class="nav-collapse">
                <!-- sidebar menu start-->
                <div class="leftside-navigation">
                    <ul class="sidebar-menu" id="nav-accordion">
                        <li>
                            <a class="active" href="/Admin/Dashboard/Index">
                                <i class="fa fa-dashboard"></i>
                                <span>Dashboard</span>
                            </a>
                        </li>
                        <li class="sub-menu">
                            <a href="/Admin/Adverts/Index">
                                <i class="fa fa-bullhorn"></i>
                                <span>Elanlar</span>
                            </a>
                            <ul class="sub">
                                <li><a href="/Admin/Adverts/Index">Bütün Elanlar</a></li>
                                <li><a href="/Admin/Adverts/OnlyVip">Vip Elanlar</a></li>
                                <li><a href="/Admin/Adverts/OnlyNew">Satılıq Elanlar</a></li>
                                <li><a href="/Admin/Adverts/OnlyRent">Kirayə Elanlar</a></li>
                            </ul>
                        </li>
                        <li>
                            <a href="/Admin/Users/Index">
                                <i class="fa fa-user"></i>
                                <span>Istifadəçilər</span>
                            </a>
                        </li>
                        <li class="sub-menu">
                            <a href="/Admin/Adverts/Index">
                                <i class="fa fa-envelope-o"></i>
                                <span>Mesajlar</span>
                            </a>
                            <ul class="sub">
                                <li><a href="/Admin/Messages/Allmessage">Bütün Mesajlar</a></li>
                                <li><a href="/Admin/Messages/Unread">Oxunmamış Mesajlar</a></li>
                                <li><a href="/Admin/Messages/Read">Oxunmuş Mesajlar</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
                <!-- sidebar menu end-->
            </div>
        </aside>
        <!--sidebar end-->
        <!--main content start-->
        <section id="main-content">
            <section id="wrpr" class="wrapper">
                @RenderBody()
            </section>
            <!-- footer -->
            <!-- / footer -->
        </section>
        <!--main content end-->
    </section>
    <script src="~/Areas/Admin/Public/js/bootstrap.js"></script>
    <script src="~/Areas/Admin/Public/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="~/Areas/Admin/Public/js/scripts.js"></script>
    <script src="~/Areas/Admin/Public/js/jquery.slimscroll.js"></script>
    <script src="~/Areas/Admin/Public/js/jquery.nicescroll.js"></script>
    <!--[if lte IE 8]><script language="javascript" type="text/javascript" src="~/Areas/Admin/Public/js/flot-chart/excanvas.min.js"></script><![endif]-->
    <script src="~/Areas/Admin/Public/js/jquery.scrollTo.js"></script>
    <!-- morris JavaScript -->

    <script>
        $(document).ready(function () {
            //BOX BUTTON SHOW AND CLOSE
            jQuery('.small-graph-box').hover(function () {
                jQuery(this).find('.box-button').fadeIn('fast');
            }, function () {
                jQuery(this).find('.box-button').fadeOut('fast');
            });
            jQuery('.small-graph-box .box-close').click(function () {
                jQuery(this).closest('.small-graph-box').fadeOut(200);
                return false;
            });

            //CHARTS
            function gd(year, day, month) {
                return new Date(year, month - 1, day).getTime();
            }



        });
    </script>
    <!-- calendar -->
    <script>
        $(function () {
            $.ajax({
                url: "/Admin/AJAX/LoadMessages",
                type: "GET",
                success: function (res) {
                    $("#header_inbox_bar").html(res)
                }
            })
        })
    </script>
    <script>
        $(function() {
            $(document).on("keypress", "#usersrc", function (e) {

                if (e.which==13) {
                    var adminsrc = $("#usersrc").val().trim();
                    if (adminsrc) {
                        $.ajax({
                            url: "/Admin/AJAX/Adminsrc",
                            data: { keyword: adminsrc },
                            type: "GET",
                            success: function (res) {
                                $("#wrpr").html(res)
                            }
                        })
                    }

                }

               
                
            })
        })
    </script>
    <script type="text/javascript" src="~/Areas/Admin/Public/js/monthly.js"></script>
    <script type="text/javascript">
        $(window).load(function () {

            $('#mycalendar').monthly({
                mode: 'event',

            });

            $('#mycalendar2').monthly({
                mode: 'picker',
                target: '#mytarget',
                setWidth: '250px',
                startHidden: true,
                showTrigger: '#mytarget',
                stylePast: true,
                disablePast: true
            });

            switch (window.location.protocol) {
                case 'http:':
                case 'https:':
                    // running on a server, should be good.
                    break;
                case 'file:':
                    alert('Just a heads-up, events will not work when run locally.');
            }

        });
    </script>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>

    @RenderSection("scripts", false)

    <!-- //calendar -->
</body>
</html>
